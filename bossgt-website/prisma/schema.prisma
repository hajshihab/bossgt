// This is your Prisma schema file for SQLite
// You can add more fields and relations as needed

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Product {
  id        Int      @id @default(autoincrement())
  name      String
  category  String
  specs     String[]
  images    String[]
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
